<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AlertDialogTheme" parent="Theme.AppCompat.DayNight.Dialog.Alert">
    <item name="colorAccent">@color/colorAccent</item>
    <item name="android:background">@color/alert_dialog_bg_color</item>
    <item name="android:textAllCaps">false</item>
  </style>
    <style name="BaseTheme" parent="Theme.AppCompat.DayNight"/>
    <style name="DialogStyle" parent="Theme.AppCompat.DayNight.Dialog">
    <item name="android:windowNoTitle">true</item>
  </style>
    <style name="SettingsActionBar" parent="@style/ThemeOverlay.AppCompat.ActionBar">
    <item name="android:textColorPrimary">@color/actionBarTextColorPrimary</item>
    <item name="android:background">@color/settings_bg_color</item>
  </style>
    <style name="SettingsActionBarTheme" parent="@style/ThemeOverlay.AppCompat.ActionBar">
    <item name="android:textColorPrimary">@color/settings_primary_text</item>
    <item name="android:background">@color/settings_background</item>
  </style>
    <style name="SettingsTheme" parent="Theme.AppCompat.DayNight">
    <item name="colorAccent">@color/colorAccent</item>
    <item name="actionBarTheme">@style/SettingsActionBarTheme</item>
  </style>
    <style name="TalkBackSettings" parent="Theme.MaterialComponents.DayNight.Bridge">
    <item name="colorAccent">@color/colorAccent</item>
    <item name="actionBarTheme">@style/SettingsActionBar</item>
  </style>
</resources>